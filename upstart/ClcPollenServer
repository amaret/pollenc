start on runlevel [2345]
stop on runlevel [06]


script
  exec start-stop-daemon --start --quiet --chuid root --exec /home/pops/bin/pollenc_server
  exec start-stop-daemon --start --quiet --chuid root --exec /usr/local/bin/riemann-health -- --host 10.239.35.22 --tag riemann-health 
end script

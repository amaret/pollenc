#!/bin/bash

#start this job with ZERO PollenEfm32GccWorkers running

ssh_worker_cntl stop all > /dev/null 2>&1

for i in {1..10}
do
  ssh_worker_cntl start $i
  export INFO="Test with $i worker, no network transfer, run pollen.jar only, instance type t2.small, 2GB RAM"
  testClcMain "${INFO}"
done

#!/bin/bash
# Annoyingly, I could not get passing the SRC as a parameter via ssh to work
export SRC=ClcPollenGccWorker
i="0"
while [ $i -lt 10 ]
  do
    export DEST=/etc/init/${SRC}_${i}.conf
    sudo cp /home/pops/$SRC $DEST
  i=$[$i+1]
done
rm /home/pops/$SRC

export SRC=riemann-health.conf
export DEST=/etc/init/${SRC}
sudo mv /home/pops/$SRC $DEST
